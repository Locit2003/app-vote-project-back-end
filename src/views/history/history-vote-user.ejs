<%-include ('../header') %>

  <!-- Content wrapper -->
  <div class="content-wrapper">
    <!-- Content -->

    <div class="container-xxl flex-grow-1 container-p-y">
      <h4 class="fw-bold py-3 mb-4">
        <span class="text-muted fw-light">Tables /</span> Account
      </h4>

      <div class="card">
        <div class="table-responsive text-nowrap">
          <table class="table">
            <thead>
              <tr >
                <th >Id</th>
                <th >Email</th>
                <th >name candidate</th>
                <th >created_time</th>
                <th >quantityVoted</th>
              </tr>
            </thead>
            <tbody class="table-border-bottom-0">
              <% history.forEach((item,key) => { %>
                <tr>
                  <td>
                    <%= item.id %>
                  </td>
                  <td>
                    <%= item.acc.email %>
                  </td>
                  <td>
                    <i class="fab fa-angular fa-lg text-danger me-3"></i>
                    <strong>
                      <%= item.candidate.name %>
                    </strong>
                  </td>
                  <td>
                    <span class="badge bg-label-primary me-1">
                        <%= item.created_time.toLocaleDateString('zh-Hans-CN') %>
                    </span>
                  </td>
                  <td>
                    <span class="badge bg-label-primary me-1">
                        <%= item.quantityVoted %>
                    </span>
                  </td>
                 
                  
                </tr>
                <% }) %>
            </tbody>
          </table>
        
        </div>
      </div>
    </div>

    <%-include ('../footer') %>
  </div>