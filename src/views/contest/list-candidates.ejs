<%-include ('../header') %>


    <!-- Content wrapper -->
    <div class="content-wrapper">
        <!-- Content -->

        <div class="container-xxl flex-grow-1 container-p-y">
            <div class="card mb-4">
                <div class="row" style="position: relative;">
                    <div class="col-md-8"><h5 class="card-header">List Candidates of the <%= names.name %></h5></div>
                    <div class="col-md-4" style="position: absolute;right:-270px"><h5 class="card-header">Total vote</h5></div>
                </div>
                <div class="demo-inline-spacing ">
                    <ul class="list-group">
                        <% candidate_by_contest.forEach((item, key) => { %>
                        <li class="list-group-item d-flex justify-content-between align-items-center">
                            <%= item.name %>
                            <span class="badge bg-primary"><%= item.quantityVote %></span>
                        </li>
                        <% })%>
                    </ul>
                </div>
                <!--/ List group with Badges & Pills -->

            </div>

        </div>
        <!-- / Content -->

        <%-include ('../footer') %>